        });
    </script>

    <script src="https://use.fontawesome.com/63310e7380.js"></script>

    <script>
        function edit_addClass(id) {
            idSuccess = id;
        }
        /*---------- pnotify ----------*/
        var stack_bottom_right = { "dir1": "up", "dir2": "right" };
        function show_stack_bottom_right(type, title, message) {
            var opts = {
                title: title,
                text: message,
                addclass: "stack-bottomright",
                stack: stack_bottom_right,
                opacity: 1,
                delay: 1000
            };
            switch (type) {
                case 'error':
                    opts.type = "error";
                    break;
                case 'info':
                    opts.type = "info";
                    break;
                case 'success':
                    opts.type = "success";
                    break;
            }
            $.pnotify.defaults.history = false;
            $.pnotify(opts);
        }
        /*---------- End pnotify ----------*/
    </script>
    <script>
        $(".function").scroll(function () {
            sessionStorage.scrollTop = $(this).scrollTop();
        });
        $(document).ready(function () {
            if (sessionStorage.scrollTop != "undefined") {
                $(".function").scrollTop(sessionStorage.scrollTop);
            }
        });
