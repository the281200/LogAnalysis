
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>@TempData["Title"]</title>
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="~/Content/themes/admin/img/tlbondslogo.ico">
    <link href="/Content/themes/web/FM/vendor/jqvmap/css/jqvmap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/Content/themes/web/FM/vendor/chartist/css/chartist.min.css">
    <!-- Vectormap -->
    <link href="/Content/themes/web/FM/vendor/jqvmap/css/jqvmap.min.css" rel="stylesheet">
    <link href="/Content/themes/web/FM/vendor/bootstrap-select/dist/css/bootstrap-select.min.css" rel="stylesheet">
    <link href="/Content/themes/web/FM/vendor/owl-carousel/owl.carousel.css" rel="stylesheet">

    <link href="~/Content/themes/web/FM/vendor/datatables/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/Content/themes/web/FM/vendor/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet" />
    <link href="~/Content/themes/web/FM/vendor/bootstrap-select/dist/css/bootstrap-select.min.css" rel="stylesheet" />

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/themes/web/FM/scss/main.css" rel="stylesheet" />
    <link href="~/Content/themes/web/css/NewStyle.css" rel="stylesheet" />
    <link href="~/Content/themes/web/css/stylescss.css" rel="stylesheet" />



    <!-- Apex Chart -->
    @*<script src="/Content/themes/web/FM/vendor/apexchart/apexchart.js"></script>*@


    <!--Chart JS-->
    @*<script src="/Content/themes/web/FM/vendor/chart.js/Chart.bundle.min.js"></script>*@
    <script src="~/Scripts/chart.js"></script>

    
    <!--file upload-->
    <script src="/Content/themes/admin/js/plugins/fileupload/bootstrap-fileupload.min.js"></script>

  

</head>
<body>

    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="sk-three-bounce">
            <div class="sk-child sk-bounce1"></div>
            <div class="sk-child sk-bounce2"></div>
            <div class="sk-child sk-bounce3"></div>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->
    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper">

        <!--**********************************
        Nav header start
    ***********************************-->
        <div class="bgr-white">


            <div class="nav-header">
                <a href="/" class="brand-logo">
                    <img class="logo-abbr" src="~/Content/themes/web/FM/images/logotlbonds.jpg" alt="">
                    <img class="logo-compact" src="" alt="">
                    <img class="brand-title" src="~/Content/themes/web/FM/images/Logo-final-cut.jpg" alt="">


                </a>

                <div class="nav-control">
                    <div class="hamburger">
                        <span class="line"></span><span class="line"></span><span class="line"></span>
                    </div>
                </div>
            </div>

            <!--**********************************
        Nav header end
    ***********************************-->
            <!--**********************************
        Chat box start
    ***********************************-->
            <!--**********************************
        Chat box End
    ***********************************-->
            <!--**********************************
        Header start
    ***********************************-->

            <div class="header">

                <div class="header-content">
                    <nav class="navbar navbar-expand">
                        <div class="text-center-header">HỆ THỐNG QUẢN LÝ DANH MỤC TÀI SẢN</div>
                        <div class="collapse navbar-collapse justify-content-between">
                            <ul class="navbar-nav header-right">

                                @Html.Action("_Info", "Account", new { @area = "" })
                            </ul>
                        </div>
                    </nav>
                </div>
            </div>
        </div>
        <!--**********************************
        Header end ti-comment-alt
    ***********************************-->
        <!--**********************************
        Sidebar start
    ***********************************-->
        <div class="bgr-white">
            @Html.Action("_MainUser", "Navigation", new { key = "user_main" })
        </div>

        <!--**********************************
        Sidebar end
    ***********************************-->
        <!--**********************************
        Content body start
    ***********************************-->
        <div class="content-body min-height-dashboard">
            <!-- row -->
            <div class="container-fluid">
                @RenderBody()
            </div>
        </div>
        <!--**********************************
        Content body end
    ***********************************-->
        <!--**********************************
        Footer start
    ***********************************-->
        @*<div class="footer">
            <div class="copyright">
                <p>Copyright © Designed &amp; Developed by Galatech 2023</p>
            </div>
        </div>*@
        <!--**********************************
        Footer end
    ***********************************-->
        <!--**********************************
       Support ticket button start
    ***********************************-->
        <!--**********************************
       Support ticket button end
    ***********************************-->
    </div>
    <!--**********************************
        Main wrapper end
    ***********************************-->
    <!--**********************************
        Scripts
    ***********************************-->
    <!-- Required vendors -->
    <script src="/Content/themes/web/FM/vendor/global/global.min.js"></script>
    <script src="/Content/themes/web/FM/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
    <script src="/Content/themes/web/FM/js/custom.min.js"></script>
    <script src="/Content/themes/web/FM/js/deznav-init.js"></script>
    <script src="/Content/themes/web/FM/vendor/owl-carousel/owl.carousel.js"></script>

    <!-- Chart piety plugin files -->
    <script src="/Content/themes/web/FM/vendor/peity/jquery.peity.min.js"></script>

    <!--DataTabe JS-->
    <script src="~/Content/themes/web/FM/vendor/datatables/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/themes/web/js/datatables.init.js"></script>

    <!--DateRangerPicker-->
    <script src="~/Content/themes/web/FM/vendor/moment/moment.min.js"></script>
    <script src="~/Content/themes/web/FM/vendor/bootstrap-daterangepicker/daterangepicker.js"></script>
    <script src="~/Content/themes/web/FM/js/plugins-init/bs-daterange-picker-init.js"></script>
    <script>
        function carouselReview() {
            /*  testimonial one function by = owl.carousel.js */
            /*  testimonial one function by = owl.carousel.js */
            jQuery('.testimonial-one').owlCarousel({
                loop: true,
                margin: 10,
                nav: false,
                center: true,
                dots: false,
                navText: ['<i class="fa fa-caret-left"></i>', '<i class="fa fa-caret-right"></i>'],
                responsive: {
                    0: {
                        items: 2
                    },
                    400: {
                        items: 3
                    },
                    700: {
                        items: 5
                    },
                    991: {
                        items: 6
                    },

                    1200: {
                        items: 4
                    },
                    1600: {
                        items: 5
                    }
                }
            })
        }

        jQuery(window).on('load', function () {
            setTimeout(function () {
                carouselReview();
            }, 1000);
        });
    </script>

</body>
</html>
